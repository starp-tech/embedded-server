{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2022",
    "importHelpers": true,
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "resolveJsonModule":true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/*.ts",
  ],
  "exclude": [
    "node_modules"
  ]
}
